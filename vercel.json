{
  "rewrites": [
    {
      "source": "/(\\d{4}-\\d{2}-\\d{2})",
      "destination": "/apps/$1/index.html"
    },
    {
      "source": "/(\\d{4}-\\d{2}-\\d{2})/",
      "destination": "/apps/$1/index.html"
    },
    {
      "source": "/(\\d{4}-\\d{2}-\\d{2})/(.*)",
      "destination": "/apps/$1/$2"
    }
  ],
  "headers": [
    {
      "source": "/apps/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
